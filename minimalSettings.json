{
    "workbench.colorTheme": "Min Dark",
    "symbols.hidesExplorerArrows": false,
    "window.zoomLevel": 1,
    "editor.fontFamily": "'JetBrains Mono'",
    "editor.fontSize": 14,
    "editor.lineHeight": 1.8,
    "workbench.startupEditor": "newUntitledFile",
    "editor.renderLineHighlight": "gutter",
    "editor.fontLigatures": true,
    "workbench.editor.labelFormat": "short",
    "explorer.compactFolders": false,
    "editor.semanticHighlighting": false,
    "breadcrumbs.enabled": false,
    "editor.minimap.enabled": false,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "workbench.statusBar.visible": false,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "package.json": ".eslint*, prettier*, tsconfig*, vite*, pnpm-lock*, package-lock*, bun.lockb",
        "tailwind.config*": "tailwind.config*, postcss.config*",
        ".env.local": ".env*",
        ".env": ".env*"
    },
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",

    // Configurações de Sons e Acessibilidade de myDefaultSettings.json
    "sound.enable": false,
    "window.enableSound": false,
    "editor.accessibilitySupport": "on",
    "accessibility.signals.lineHasError": {
        "sound": "off"
    },
    "accessibility.signals.lineHasBreakpoint": {
        "sound": "off"
    },
    "accessibility.signals.lineHasInlineSuggestion": {
        "sound": "off"
    },
    "accessibility.signals.terminalQuickFix": {
        "sound": "off"
    },
    "accessibility.signals.onDebugBreak": {
        "sound": "off"
    },
    "accessibility.signals.noInlayHints": {
        "sound": "off"
    },
    "accessibility.signals.taskCompleted": {
        "sound": "off"
    },
    "accessibility.signals.taskFailed": {
        "sound": "off"
    },
    "accessibility.signals.notebookCellCompleted": {
        "sound": "off"
    },
    "accessibility.signals.notebookCellFailed": {
        "sound": "off"
    },
    "accessibility.signals.diffLineInserted": {
        "sound": "off"
    },
    "accessibility.signals.diffLineDeleted": {
        "sound": "off"
    },

    // Configurações de Formatação de myDefaultSettings.json
    "editor.formatOnSave": true,
    "[javascript]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[javascriptreact]": {
        "editor.formatOnSave": false
    },
    "[typescript]": {
        "editor.formatOnSave": false
    },
    "[typescriptreact]": {
        "editor.formatOnSave": false
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.tabSize": 4,
    "editor.formatOnType": true,
    "editor.renderWhitespace": "all",
    "editor.wordWrapColumn": 120,
    "editor.wordWrap": "wordWrapColumn",
    "editor.rulers": [
        120
    ],
    "[xml]": {
        "editor.defaultFormatter": "DotJoshJohnson.xml"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[python]": {
        "editor.formatOnType": true
    },

    // Configurações de Integrações de myDefaultSettings.json
    "editor.inlineSuggest.enabled": true,
    "gitHub.copilot.inlineSuggest.enable": true,
    "gitHub.copilot.enable": true,
    "terminal.integrated.shellIntegration.enabled": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "git.autofetch": true,
    "python.languageServer": "Pylance",
    "tabnine.experimentalAutoImports": true,
    "redhat.telemetry.enabled": true,
    "sonarlint.rules": {
        "python:S125": {
            "level": "off"
        }
    },
    "java.debug.settings.console": "internalConsole",
    "eslint.lintTask.enable": true,
    "eslint.codeAction.showDocumentation": {
        "enable": true
    },
    "jdk.jdkhome": "c:\\Program Files\\Java\\jdk-21",
    "sqldeveloper.sqlHistory.historyLimit": 500,
    "chat.instructionsFilesLocations": {
        ".github/instructions": true,
        "C:\\Users\\breno\\AppData\\Local\\Temp\\postman-collections-post-response.instructions.md": true,
        "C:\\Users\\breno\\AppData\\Local\\Temp\\postman-collections-pre-request.instructions.md": true,
        "C:\\Users\\breno\\AppData\\Local\\Temp\\postman-folder-post-response.instructions.md": true,
        "C:\\Users\\breno\\AppData\\Local\\Temp\\postman-folder-pre-request.instructions.md": true,
        "C:\\Users\\breno\\AppData\\Local\\Temp\\postman-http-request-post-response.instructions.md": true,
        "C:\\Users\\breno\\AppData\\Local\\Temp\\postman-http-request-pre-request.instructions.md": true,
        "/tmp/postman-collections-post-response.instructions.md": true,
        "/tmp/postman-collections-pre-request.instructions.md": true,
        "/tmp/postman-folder-post-response.instructions.md": true,
        "/tmp/postman-folder-pre-request.instructions.md": true,
        "/tmp/postman-http-request-post-response.instructions.md": true,
        "/tmp/postman-http-request-pre-request.instructions.md": true
    },
    "vs-kubernetes": {
        "vscode-kubernetes.helm-path-linux": "/home/cssbreno/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm",
        "vscode-kubernetes.kubectl-path-linux": "/home/cssbreno/.local/state/vs-kubernetes/tools/kubectl/kubectl",
        "vscode-kubernetes.minikube-path-linux": "/home/cssbreno/.local/state/vs-kubernetes/tools/minikube/linux-amd64/minikube"
    },
    "docker.extension.enableComposeLanguageServer": false,
    "trae.tab.enableAutoImport": true,
    "trae.tab.enableAutoRename": true,
    "editor.linkedEditing": true,
    "window.newWindowProfile": "Perfil minimalista",
    "window.menuBarVisibility": "classic"
}